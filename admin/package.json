    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^22.10.7",
    "sass-embedded": "^1.83.4",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "unplugin-icons": "^22.0.0",
    "vite": "^5.4.11",
    "wrangler": "^3.101.0"
